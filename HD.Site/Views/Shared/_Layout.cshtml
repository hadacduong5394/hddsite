
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>HD | @ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link href="/Scripts/plugins/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Scripts/plugins/css/toastr.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link href="/Scripts/plugins/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="/Scripts/plugins/css/blue.css" rel="stylesheet" />

    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="/Scripts/plugins/css/_all-skins.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="/Scripts/plugins/css/Pagination.css" rel="stylesheet" />
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <header class="main-header">
            @RenderPage("_Top.cshtml")
        </header>
        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        @RenderPage("_Left.cshtml")
        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.3.6
            </div>
            <strong>Copyright &copy; 2014-2016 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights
            reserved.
        </footer>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.3 -->
    <script src="/Scripts/plugins/js/jquery-2.2.3.min.js"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="/Scripts/plugins/js/bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="/Scripts/plugins/js/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="/Scripts/plugins/js/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Scripts/plugins/js/app.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/Scripts/plugins/js/demo.js"></script>
    <script src="/Scripts/plugins/js/icheck.min.js"></script>
    <script src="/Scripts/plugins/js/toastr.min.js"></script>
    <script src="/Scripts/plugins/js/toastrServiceSetting.js"></script>
    <script src="/Scripts/plugins/customJs/logOffController.js"></script>
    <script>
        $(function () {
            $('input').iCheck({
                checkboxClass: 'icheckbox_square-blue',
                radioClass: 'iradio_square-blue',
                increaseArea: '20%' // optional
            });
        });
    </script>
    @if (TempData["toastrType"] != null && TempData["toastrMessage"] != null)
    {
        <script>
            var type = '@TempData["toastrType"]';
            var message = '@TempData["toastrMessage"]';
            if (type === 'success') {
                toastr.success(message);
                $.ajax({
                    url: "/Base/ClearAlert",
                    type: "post",
                    dataType: "json"
                });
            }

            if (type === 'info') {
                toastr.info(message);
                $.ajax({
                    url: "/Base/ClearAlert",
                    type: "post",
                    dataType: "json"
                });
            }

            if (type === 'warning') {
                toastr.warning(message);
                $.ajax({
                    url: "/Base/ClearAlert",
                    type: "post",
                    dataType: "json"
                });
            }

            if (type === 'error') {
                toastr.error(message);
                $.ajax({
                    url: "/Base/ClearAlert",
                    type: "post",
                    dataType: "json"
                });
            }
        </script>
    }

    @RenderSection("JsFooter", false)
</body>
</html>
